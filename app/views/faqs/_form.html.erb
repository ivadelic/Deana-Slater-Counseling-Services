<main>
  <%= form_for @faq do |f| %>
  <div class="abouts-form">
    <div class="field">

      <%= f.label :question %>
      <%= f.text_field :question %>

      <%= f.label :answer %>
      <%= f.text_field :answer %>
    </div>

    <div class="submit-button">
      <%= f.submit %>
    </div>
  </div>
  <% end %>
</main>